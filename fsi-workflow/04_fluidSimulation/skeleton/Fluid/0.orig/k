FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

//internalField   uniform 0.0003;
internalField   uniform 0.00135;

boundaryField
{

    //#includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    far
    {
        type            zeroGradient;
    }
    
    naca2312
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    
    
    symmetry
    {
        type            symmetryPlane;
    }
    
}

