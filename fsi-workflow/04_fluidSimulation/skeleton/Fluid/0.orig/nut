FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0.0020125;

boundaryField
{

    // #includeEtc "caseDicts/setConstraintTypes"

    inlet
    {
        type            fixedValue;
        value           $internalField;
    }

    outlet
    {
        type            zeroGradient;
    }

    far
    {
        type            zeroGradient;
    }

    naca2312
    {
        type            nutkWallFunction;
        value           $internalField;
    }

    symmetry
    {
        type            symmetryPlane;
    }
}

