FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    system;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         snappyHexMesh;

//libs ( "libOpenFOAM.so" "libgroovyBC.so" ) ;
libs ( "libOpenFOAM.so" ) ;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime              5;

deltaT              0.001;

writeControl        adjustableRunTime;

writeInterval       0.1;

purgeWrite          50;

writeFormat         ascii;

writePrecision      16;

writeCompression    on;

timeFormat          general;

timePrecision       12;

runTimeModifiable   true;
